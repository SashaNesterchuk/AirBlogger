        "jsdom-global": "^3.0.2",
        "laravel-mix": "^5.0.5",
        "mocha": "^7.1.2",
        "mocha-webpack": "^2.0.0-beta.0",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "sinon": "^9.0.3",
        "sinon-chai": "^3.5.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
